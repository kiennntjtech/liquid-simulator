{
  "name": "liquid-simulator",
  "version": "0.0.1",
  "description": "liquid simulator service",
  "author": "TJTech",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max_old_space_size=8192 dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:fix": "prettier --write ./**/*.{ts,js,json,*rc}",
    "lint": "npm run eslint:fix && npm run prettier:fix",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.385.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-ws": "^10.3.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/passport": "^1.0.12",
    "@types/ws": "^8.5.10",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.9",
    "decimal.js": "^10.4.3",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nanoid": "^3.3.6",
    "nestjs-cls": "^3.5.1",
    "node-stream-zip": "^1.15.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}